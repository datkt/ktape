{
  "name": "@datkt/tape",
  "version": "0.3.1",
  "description": "TAP output producing test runner for Kotlin",
  "src": [
    "assert.kt",
    "callback.kt",
    "constants.kt",
    "context.kt",
    "index.kt",
    "LICENSE",
    "Makefile",
    "noop.kt",
    "options.kt",
    "package.json",
    "README.md",
    "result.kt",
    "results.kt",
    "stream.kt",
    "tape.def",
    "tape.h",
    "test.kt",
    "truthy.kt"
  ],
  "directories": {},
  "scripts": {
    "tests": "NPM=1 make test 2>&1>/dev/null && ./test/test.kexe",
    "test": "NPM=1 make test 2>&1>/dev/null && ./test/test.kexe | tap-markdown",
    "postinstall": "konanc *.kt -output tape -produce library"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datkt/tape.git"
  },
  "keywords": [
    "datkt",
    "dat",
    "kotlin",
    "tape",
    "tap",
    "test",
    "runner"
  ],
  "author": "Joseph Werle <werle@littlstar.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datkt/tape/issues"
  },
  "homepage": "https://github.com/datkt/tape#readme",
  "devDependencies": {
    "@extendscripter/tap-markdown": "^1.3.1"
  },
  "dependencies": {}
}
